<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="style.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@1,400;1,700&display=swap" rel="stylesheet">
    <title>Full Stack App</title>
</head>
<body>
    <nav>
        <div class="icon nav-icon-3">
        <span></span>
        <span></span>
        <span></span>
         </div>
         <ul class="hidden" >
            <li>General</li>
            <li>Fitness</li>
            <li>Work</li>
            <li>School</li>
            <li>Hype</li>
         </ul>

    </nav>
    <div class="container">
        <h1>Your motivational quote for today:</h1>
        <div id="quote">
        <% let quote = quotes[Math.floor(Math.random()*quotes.length)] %>
            <p class = "quote hide"><%= quote.quote %></p>
            <span class = "name hide"><%= quote.author %></span>
        </div>
        <button id="main">Go Get</button>
    </div>
</body>
<script src="main.js" type="text/javascript"></script>
</html>